  </documentation>
  <wsdl:types>
    <xs:schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="urn://epages.de/WebService/ChangeLogService/2014/06">
      <xs:import namespace="http://schemas.xmlsoap.org/soap/encoding/" />
      <xs:import namespace="http://schemas.xmlsoap.org/wsdl/" />
      <xs:complexType name="TFindCreates_Return">
        <xs:all>
          <xs:element name="Deletes" type="tns:TListOfFindCreates" />
          <xs:element name="LatestCreate" type="xs:dateTime" />
        </xs:all>
      </xs:complexType>
      <xs:complexType name="TListOfFindCreates">
        <xs:complexContent mixed="false">
          <xs:restriction base="soapenc:Array">
            <xs:attribute wsdl:arrayType="tns:TFindCreates[]" ref="soapenc:arrayType" />
          </xs:restriction>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="TFindCreates">
        <xs:all>
          <xs:element minOccurs="0" name="Path" type="xs:string" />
          <xs:element minOccurs="0" name="GUID" type="xs:string" />
        </xs:all>
      </xs:complexType>
      <xs:complexType name="TFindDeletes_Return">
        <xs:all>
          <xs:element name="Deletes" type="tns:TListOfFindDeletes" />
          <xs:element name="LatestDelete" type="xs:dateTime" />
        </xs:all>
      </xs:complexType>
      <xs:complexType name="TListOfFindDeletes">
        <xs:complexContent mixed="false">
          <xs:restriction base="soapenc:Array">
            <xs:attribute wsdl:arrayType="tns:TFindDeletes[]" ref="soapenc:arrayType" />
          </xs:restriction>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="TFindDeletes">
        <xs:all>
          <xs:element minOccurs="0" name="Path" type="xs:string" />
          <xs:element minOccurs="0" name="GUID" type="xs:string" />
        </xs:all>
      </xs:complexType>
      <xs:complexType name="TFindUpdates_Return">
        <xs:all>
          <xs:element name="Updates" type="tns:TListOfFindUpdates" />
          <xs:element name="LatestUpdate" type="xs:dateTime" />
        </xs:all>
      </xs:complexType>
      <xs:complexType name="TListOfFindUpdates">
        <xs:complexContent mixed="false">
          <xs:restriction base="soapenc:Array">
            <xs:attribute wsdl:arrayType="tns:TFindUpdates[]" ref="soapenc:arrayType" />
          </xs:restriction>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="TFindUpdates">
        <xs:all>
          <xs:element minOccurs="0" name="Path" type="xs:string" />
          <xs:element minOccurs="0" name="GUID" type="xs:string" />
        </xs:all>
      </xs:complexType>
    </xs:schema>
  </wsdl:types>
  <wsdl:message name="msg_findDeletes_Input">
    <wsdl:part name="DeletedAfter" type="xs:dateTime" />
    <wsdl:part name="Class" type="xs:string" />
  </wsdl:message>
  <wsdl:message name="msg_findDeletes_Output">
    <wsdl:part name="DeleteSet" type="tns:TFindDeletes_Return" />
  </wsdl:message>
  <wsdl:message name="msg_findCreates_Input">
    <wsdl:part name="CreatedAfter" type="xs:dateTime" />
    <wsdl:part name="Class" type="xs:string" />
