      </wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="findUpdates">
      <soap:operation soapAction="urn://epages.de/WebService/ChangeLogService/2014/06#findUpdates" />
      <wsdl:input>
        <soap:body use="encoded" namespace="urn://epages.de/WebService/ChangeLogService/2014/06" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" />
      </wsdl:input>
      <wsdl:output>
        <soap:body use="encoded" namespace="urn://epages.de/WebService/ChangeLogService/2014/06" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" />
      </wsdl:output>
    </wsdl:operation>
